<?xml version="1.0"?>
<extension key="org.ndi.chileprovinces" type="module">
  <file>chileprovinces</file>
  <name>Chile Provinces Loader</name>
  <description>Loads Provinces for Chile</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Evan Summers</author>
    <email>esummers@ndi.org</email>
  </maintainer>
  <releaseDate>2019-05-16</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <urls>
    <url desc="documentation">http://aghstrategies.com/counties</url>
  </urls>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Use this extension to load Provinces for Chile.  After installation, this extension does nothing (and can even be disabled).</comments>
  <civix>
    <namespace>CRM/Chileprovinces</namespace>
  </civix>
</extension>
